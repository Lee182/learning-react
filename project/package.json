{
  "name": "personal-details-form",
  "version": "1.0.0",
  "description": "simple web form parsing, tobe connected to mongodb backend",
  "main": "server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-form": "node shared/parseForm.js",
    "serve": "node server/server.js",
    "build-html": "cp client/index.html dist/index.html",
    "build-js": "webpack",
    "watch-js": "webpack -w",
    "watch-stylus": "stylus -w client/styl.styl -o dist",
    "dev": "npm-run-all -p serve watch-stylus watch-js build-html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lee182/personal-details-form.git"
  },
  "keywords": [
    "react",
    "mongodb",
    "forms",
    "cson"
  ],
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "flat": "^2.0.1",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "cson": "^4.1.0",
